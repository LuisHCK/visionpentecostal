
 <% link_edit = edit_program_path(@program)%>

<div class="container-fixed">
		<h2>
			 <%= @program.titulo %>
		</h2>

		<%img = @program.portada%>
		<img src="<%= img %>">

		<p>
			<%= @program.descripcion %>
		</p>

 <% if admin_signed_in?%>

  <div class="fixed-action-btn" style="bottom: 45px; right: 24px;">
    <a class="btn-floating btn-large red">
      <i class="large material-icons">mode_edit</i>
    </a>
    <ul>
      <li><a href="#0" class="cd-popup-trigger btn-floating red"><i class="material-icons">delete</i></i></a></li>
      <li><a href="<%=link_edit%>" class="btn-floating yellow darken-1"><i class="material-icons">code</i></a></li>
      <li><a href="/programs/new" class="btn-floating green"><i class="material-icons">publish</i></a></li>
    </ul>
  </div>
<a href="#0" class="cd-popup-trigger">View Pop-up</a>

<%= render "/layouts/popup" %>


<% end -%>